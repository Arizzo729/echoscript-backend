{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "Vite: dev",
      "type": "npm",
      "script": "dev",
      "options": { "cwd": "${workspaceFolder}/Frontend" },
      "isBackground": true,
      "problemMatcher": {
        "owner": "custom",
        "pattern": [
          { "regexp": ".*", "file": 1, "location": 2, "message": 0 }
        ],
        "background": {
          "activeOnStart": true,
          "beginsPattern": "vite v\\d+\\.\\d+\\.\\d+",
          "endsPattern": "Local:\\s+http://localhost:5173"
        }
      }
    }
  ]
}
